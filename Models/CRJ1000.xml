	<!-- Door animations -->
	<animation>
		<type>rotate</type>
		<object-name>RightDoor</object-name>
		<object-name>RightDoorInt</object-name>
		<property>sim/model/door-positions/pax-right/position-norm</property>
		<factor>180</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
		<center>
			<x-m>-14.352</x-m>
			<y-m>1.39</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

