		<object-name>AileronLWire</object-name>
		<property>surface-positions/left-aileron-pos-norm</property>
		<factor>20</factor>
		<axis>
			<x1-m>3.854</x1-m>
			<y1-m>-9.955</y1-m>
			<z1-m>-1.889</z1-m>
			<x2-m>3.163</x2-m>
			<y2-m>-8.133</y2-m>
			<z2-m>-1.945</z2-m>
		</axis>
	</animation>
	<animation>
		<name>Right aileron</name>
