<?xml version="1.0" encoding="UTF-8" ?>


<PropertyList>
	<title>Pre-start</title>
	<item>
		<name>Parking Brake</name>
		<value>SET</value>
		<condition>
			<equals>
				<property>/controls/gear/brake-parking</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/gear/brake-parking</property>
			<value>1</value>
		</binding>
	</item>
	<item>
		<name>Throttle</name>
		<value>IDLE</value>
		<condition>
			<and>
				<not><property>/controls/engines/engine[0]/cutoff</property></not>
				<not><property>/controls/engines/engine[1]/cutoff</property></not>
				<less-than>
					<property>/controls/engines/engine[0]/throttle</property>
					<value>0.02</value>
				</less-than>
				<less-than>
					<property>/controls/engines/engine[1]/throttle</property>
					<value>0.02</value>
				</less-than>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[0]/cutoff</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[1]/cutoff</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>Landing gear lever</name>
		<value>CHECK DOWN</value>
		<condition>
			<equals>
				<property>/controls/gear/gear-down</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/gear/gear-down</property>
			<value>1</value>
		</binding>
	</item>
	<!--item>
		<name>Generator 1 and 2</name>
		<value>OFF</value>
		<condition>
			<and>
				<equals>
					<property>/controls/electric/engine[0]/generator</property>
					<value>0</value>
				</equals>
				<equals>
					<property>/controls/electric/engine[1]/generator</property>
					<value>0</value>
				</equals>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/electric/engine[0]/generator</property>
			<value>0</value>
		</binding>	
		<binding>
			<command>property-assign</command>
			<property>/controls/electric/engine[1]/generator</property>
			<value>0</value>
		</binding>	
	</item-->
	<item>
		<name>Battery Master</name>
		<value>ON</value>
		<condition>
			<equals>
				<property>/controls/electric/battery-switch</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/electric/battery-switch</property>
			<value>1</value>
		</binding>	
	</item>
	<item>
		<name>Panel lights</name>
		<value>ON if required</value>
	</item>
	<item>
		<name>Nav lights</name>
		<value>ON</value>
		<condition>
			<equals>
				<property>/controls/lighting/nav-lights</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/lighting/nav-lights</property>
			<value>1</value>
		</binding>	
	</item>
	<!--item>
		<name>Avionics Master</name>
		<value>ON</value>
	</item-->

	<item>
		<name>Fuel Quantity</name>
		<value>CHECK</value>
	</item>
	<item>
		<name>Request Fuel</name>
		<value>AS REQUIRED</value>
	</item>
	<item>
		<name>Beacon</name>
		<value>ON</value>
		<condition>
			<equals>
				<property>/controls/lighting/beacon</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/lighting/beacon</property>
			<value>1</value>
		</binding>	
	</item>
	<item>
		<name>APU</name>
		<value>START</value>
		<condition>
			<or>
				<and>
					<equals>
						<property>/controls/APU/electronic-control-unit</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/controls/APU/off-on</property>
						<value>1</value>
					</equals>				
				</and>
			
			</or>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/APU/electronic-control-unit</property>
			<value>1</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/APU/off-on</property>
			<value>1</value>
		</binding>
	</item>
	<item>
		<name>APU/Generator</name>
		<value>ON</value>
		<condition>
			<equals>
				<property>/controls/electric/APU-generator</property>
				<value>1</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/electric/APU-generator</property>
			<value>1</value>
		</binding>
	</item>
	<item>
		<name>Hydraulic pump switch</name>
		<value>CHECK ON</value>
		<condition>
			<and>
				<property>/controls/hydraulic/system[0]/pump-b</property>
				<property>/controls/hydraulic/system[1]/pump-b</property>
				<property>/controls/hydraulic/system[2]/pump-b</property>
				<property>/controls/hydraulic/system[2]/pump-a</property>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/hydraulic/system[0]/pump-b</property>
			<value>2</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/hydraulic/system[1]/pump-b</property>
			<value>2</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/hydraulic/system[2]/pump-b</property>
			<value>2</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/hydraulic/system[2]/pump-a</property>
			<value>1</value>
		</binding>
	</item>
	<item>
		<name>Flight controls</name>
		<value>SELECT ON EICAS</value>
	</item>
	<item>
		<name>Flaps</name>
		<value>UP</value>
		<condition>
			<equals>
				<property>/surface-positions/flap-pos-norm</property>
				<value>0</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/flight/flaps</property>
			<value>0</value>
		</binding>		
	</item>
	<item>
		<name>Spoilers</name>
		<value>RETRACTED</value>
	</item>
	<item>
		<name>Flight controls</name>
		<value>FREE AND CORRECT</value>
	</item>
	<item>
		<name>Engine Gauges on EICAS</name>
		<value>SELECT</value>
	</item>
	<item>
		<name>Anti-Ice Switches</name>
		<value>OFF</value>
		<condition>
			<and>
				<not><property>/controls/anti-ice/wing-heat</property></not>
				<not><property>/controls/anti-ice/engine[0]/inlet-heat</property></not>
				<not><property>/controls/anti-ice/engine[1]/inlet-heat</property></not>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/anti-ice/wing-heat</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/anti-ice/engine[0]/inlet-heat</property>
			<value>0</value>
		</binding>
		<binding>
			<command>property-assign</command>
			<property>/controls/anti-ice/engine[1]/inlet-heat</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>Passenger signs</name>
		<value>ON</value>
		<condition>
			<and>
				<not-equals>
					<property>/controls/switches/no-smoking-sign</property>
					<value>1</value>
				</not-equals>
				<not-equals>
					<property>/controls/switches/seatbelt-sign</property>
					<value>1</value>
				</not-equals>
			</and>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/switches/no-smoking-sign</property>
			<value>2</value>
		</binding>		
		<binding>
			<command>property-assign</command>
			<property>/controls/switches/seatbelt-sign</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>ATIS</name>
		<value>CHECK</value>
	</item>
	<item>
		<name>Altimeter</name>
		<value>SET</value>
	</item>
	<item>
		<name>Clearance</name>
		<value>OBTAIN</value>
	</item>
	<item>
		<name>Lights</name>
		<value>AS REQUIRED</value>
	</item>
	<item>
		<name>Doors</name>
		<value>CLOSE</value>
		<condition>
			<and>
			<not><property>/sim/model/door-positions/aft-cargo/position-norm</property></not>
			<not><property>/sim/model/door-positions/avionic/position-norm</property></not>
			<not><property>/sim/model/door-positions/ctr-cargo/position-norm</property></not>
			<not><property>/sim/model/door-positions/emer-l1/position-norm</property></not>
			<not><property>/sim/model/door-positions/emer-l2/position-norm</property></not>
			<not><property>/sim/model/door-positions/emer-r1/position-norm</property></not>
			<not><property>/sim/model/door-positions/emer-r2/position-norm</property></not>
			<not><property>/sim/model/door-positions/fwd-cargo/position-norm</property></not>
			<not><property>/sim/model/door-positions/pax-left/position-norm</property></not>
			<not><property>/sim/model/door-positions/pax-right/position-norm</property></not>
			</and>
		</condition>
		<binding>
			<command>nasal</command>
			<script>CRJ700.doors.close();</script>
		</binding>
	</item>
	<item>
		<name>Parking Brake (if pushback)</name>
		<value>RELEASE</value>
		<condition>
			<equals>
				<property>/controls/gear/brake-parking</property>
				<value>0</value>
			</equals>
		</condition>
		<binding>
			<command>property-assign</command>
			<property>/controls/gear/brake-parking</property>
			<value>0</value>
		</binding>
	</item>
	<item>
		<name>Pushback</name>
		<value>AS REQUIRED</value>
	</item>	
</PropertyList>
