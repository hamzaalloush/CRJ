<?xml version="1.0" encoding="UTF-8" ?>

<!-- Bombardier CRJ700 series -->
<!-- Aircraft property settings specific to the CRJ900LR -->

<PropertyList include="CRJ700-main.xml">

	<sim>
		<description>Bombardier CRJ900LR</description>
		<author>Ryan Miller (3d), Ryan Miller (FDM)</author>
		<status>early-production</status>
		<aircraft-version>1.1.0</aircraft-version>
		<rating>
			<FDM type="int">3</FDM>
			<systems type="int">3</systems>
			<cockpit type="int">4</cockpit>
			<model type="int">4</model>
		</rating>
		<flight-model>yasim</flight-model>
		<aero>CRJ900LR</aero>

		<model>
			<path>Aircraft/CRJ700-family/Models/CRJ900.xml</path>
			<variant type="int">2</variant>
			<door>
				<position-x-m type="float">-13.459</position-x-m>
				<position-y-m type="float">-1.139</position-y-m>
				<position-z-m type="float">-1.796</position-z-m>
				<jetway-hood-deg type="float">3</jetway-hood-deg>
			</door>
			<has-slats type="bool">true</has-slats>
		</model>

        <dimensions>
            <radius-m type="float">36.4</radius-m>
        </dimensions>

        <view n="0">
            <name>Captain View</name>
            <internal type="bool" archive="y">true</internal>
            <config>
                <x-offset-m archive="y">-0.528</x-offset-m>
                <y-offset-m archive="y">-0.468</y-offset-m>
                <z-offset-m archive="y">-15.045</z-offset-m>
            </config>
        </view>
        <view n="100">
            <name>Cabin View</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">0</x-offset-m>
                <y-offset-m archive="y">-0.468</y-offset-m>
                <z-offset-m archive="y">0</z-offset-m>
            </config>
        </view>
        <view n="101">
            <name>First Officer View</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">0.528</x-offset-m>
                <y-offset-m archive="y">-0.468</y-offset-m>
                <z-offset-m archive="y">-15.045</z-offset-m>
            </config>
        </view>
        <view n="102">
            <name>OH Panel</name>
            <type>lookfrom</type>
            <internal type="bool" >true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m>0</x-offset-m>
                <y-offset-m>-0.468</y-offset-m>
                <z-offset-m>-15.29</z-offset-m>
                <pitch-offset-deg archive="y">59.2</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <default-field-of-view-deg type="double"  archive="y">85.3</default-field-of-view-deg>
            </config>
			<limits>
				<enabled type="bool">true</enabled>
				<left>
				  <heading-max-deg type="double">0</heading-max-deg>
				</left>
				<right>
				  <heading-max-deg type="double">0</heading-max-deg>
				</right>
			</limits>
        </view>
        <view n="103">
            <name>Center Panel</name>
            <type>lookfrom</type>
            <internal type="bool" >true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m>0</x-offset-m>
                <y-offset-m>-0.468</y-offset-m>
                <z-offset-m>-15.29</z-offset-m>
                <pitch-offset-deg>-55</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <default-field-of-view-deg type="double">56</default-field-of-view-deg>
            </config>
 			<limits>
				<enabled type="bool">true</enabled>
				<left>
				  <heading-max-deg type="double">0</heading-max-deg>
				</left>
				<right>
				  <heading-max-deg type="double">0</heading-max-deg>
				</right>
			</limits>       
		</view>
        <view n="104">
            <name>Radio Panel</name>
            <type>lookfrom</type>
            <internal type="bool" >true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m>0</x-offset-m>
                <y-offset-m>-0.9</y-offset-m>
                <z-offset-m>-15.14</z-offset-m>
                <pitch-offset-deg>-64.3</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <default-field-of-view-deg type="double">77.3</default-field-of-view-deg>
            </config>
			<limits>
				<enabled type="bool">true</enabled>
				<left>
				  <heading-max-deg type="double">5</heading-max-deg>
				</left>
				<right>
				  <heading-max-deg type="double">5</heading-max-deg>
				</right>
			</limits>        
		</view>
        <view n="108">
            <name>Nose gear</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">0</x-offset-m>
                <y-offset-m archive="y">-2.46</y-offset-m>
                <z-offset-m archive="y">-11.6</z-offset-m>
                <pitch-offset-deg>-10</pitch-offset-deg>
                <heading-offset-deg>0</heading-offset-deg>
                <default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
            </config>
        </view>      
        <view n="109">
            <name>Gear</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">0</x-offset-m>
                <y-offset-m archive="y">-2.6</y-offset-m>
                <z-offset-m archive="y">-8.6</z-offset-m>
                <pitch-offset-deg>-10</pitch-offset-deg>
                <heading-offset-deg>180</heading-offset-deg>
                <default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
            </config>
        </view>      
		<view n="110">
            <name>15A</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">-1.05</x-offset-m>
                <y-offset-m archive="y">-0.59</y-offset-m>
                <z-offset-m archive="y">-0.87</z-offset-m>
                <pitch-offset-deg>-7</pitch-offset-deg>
                <heading-offset-deg>90</heading-offset-deg>
                <default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
            </config>
        </view>
		<view n="111">
            <name>23A</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">-1.05</x-offset-m>
                <y-offset-m archive="y">-0.6</y-offset-m>
                <z-offset-m archive="y">4.26</z-offset-m>
                <pitch-offset-deg>-10</pitch-offset-deg>
                <heading-offset-deg>90</heading-offset-deg>
                <default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
            </config>
        </view>
		<view n="120">
            <name>15F </name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">1.05</x-offset-m>
                <y-offset-m archive="y">-0.59</y-offset-m>
                <z-offset-m archive="y">-0.87</z-offset-m>
                <pitch-offset-deg>-7</pitch-offset-deg>
                <heading-offset-deg>270</heading-offset-deg>
                <default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
            </config>
        </view>
		<view n="121">
            <name>23F</name>
            <type>lookfrom</type>
            <internal type="bool" archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <x-offset-m archive="y">1.05</x-offset-m>
                <y-offset-m archive="y">-0.6</y-offset-m>
                <z-offset-m archive="y">4.26</z-offset-m>
                <pitch-offset-deg>-10</pitch-offset-deg>
                <heading-offset-deg>270</heading-offset-deg>
                <default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
            </config>
        </view>

        <weight n="1">
            <weight-lb>14000</weight-lb>
            <max-lb>23350</max-lb>
            <min-lb>0</min-lb>
        </weight>

        <menubar>
            <default>
                <menu n="100">
                    <label>Bombardier CRJ900LR</label>
                </menu>
            </default>
        </menubar>
    </sim>

</PropertyList>
